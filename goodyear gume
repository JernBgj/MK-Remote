        function async_load() {
          var s = document.createElement('script');
          s.type = 'text/javascript';
          s.src = ('https:' == document.location.protocol ? 'https://pi' : 'http://cdn') + '.pardot.com/pd.js';
          var c = document.getElementsByTagName('script')[0];
          c.parentNode.insertBefore(s, c);
        }
        if (window.attachEvent) { window.attachEvent('onload', async_load); } else { window.addEventListener('load', async_load, false); }
      })();
      
      
        var loadMailchimp = false;
        function loadMailchimpFunc(){
        if (!loadMailchimp){
        !function (c, h, i, m, p) { m = c.createElement(h), p = c.getElementsByTagName(h)[0], m.async = 1, m.src = i, p.parentNode.insertBefore(m, p) }(document, "script", "https://chimpstatic.com/mcjs-connected/js/users/4e19d93b1ca613143c3489322/25898da30edc604657c251f3a.js");
        loadMailchimp = true;
      }
      }

        window.addEventListener("mousemove", loadMailchimpFunc, false);
        window.addEventListener("scroll", loadMailchimpFunc, false);
      
      
        var segmentifyUrlId = 'a3a8d4af-8218-48bb-8d39-3b2cfc0f606f';
      
      
        var loadSegmentify = false;
        var loadSegmentifyScript = () => {
        if (!loadSegmentify){
        var head = document.getElementsByTagName('head')[0];
        var script = document.createElement('script');
        script.type = 'text/javascript';
        script.src = `//cdn.segmentify.com/${segmentifyUrlId}/segmentify.js`;
        script.charset = 'UTF-8';
        head.appendChild(script);
        loadSegmentify = true;
      }
      }

        window.addEventListener("mousemove", loadSegmentifyScript, false);
        window.addEventListener("scroll", loadSegmentifyScript, false);
      
</script>
