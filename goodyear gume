<script type="text/javascript">

  var cookieMessage = '<p>Ceneje.si uporablja piškotke za zagotavljanje kvalitetne uporabniške izkušnje, nemoteno delovanje vseh funkcij spletne strani, prikazovanje oglasov in merjenje ter analizo podatkov o uporabi spletne strani.</p><p>Za nemoteno uporabo naše strani se moraš strinjati z uporabo piškotkov.</p>';
    
  var loadCookies = false;
  
  function loadCookiesScript() {
    if (!loadCookies){
      setTimeout(function() {
        if (typeof jQuery == 'undefined') return;
        jQuery(function(){
          if(typeof $.cookieCuttr !== "undefined"){
            $.cookieCuttr({
              cookieAnalyticsMessage: '!',
              cookieAnalytics: false,
              cookieMessage: cookieMessage,
              cookieAcceptButtonText: "Strinjam se",
              cookieDeclineButtonText: "Nastavitve",
              cookieSettingsRedirectLink: "https://www.ceneje.si/pravila-o-zasebnosti#cookies"
            });
          }
        })
      },3500);
      loadCookies = true;
    }
  }

   window.addEventListener("mousemove",loadCookiesScript,false);
   window.addEventListener("scroll",loadCookiesScript,false);
  
</script>
<script>
  function decodeCookie() {
    let cookieParts = document.cookie.split(";"),
      cookies = {};

    for (let i = 0; i < cookieParts.length; i++) {
      let nameValue = cookieParts[i],
        equalsPos = nameValue.indexOf("="),
        name = unescape(nameValue.slice(0, equalsPos)).trim();
      cookies[name] = unescape(nameValue.slice(equalsPos + 1));
    }
    return cookies;
  }

  function getCookieValue(searchWord) {
    let cookies = this.decodeCookie();
    for (let name in cookies) {
      let value = cookies[name];
      if (name.indexOf(searchWord) === 0) {
        return value;
      }
    }
  }

  function getCookieName(searchWord) {
    let cookies = this.decodeCookie();
    for (let name in cookies) {
      if (name.match(searchWord)) {
        return name;
      }
    }
  }

  let experimentId = "rSmKGmECSo-BbqCNcfCtBg";
  let chosenVariation = parseInt(getCookieValue(getCookieName(experimentId)));
</script>
